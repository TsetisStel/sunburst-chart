{
    "name": "tsetis/sunburst-chart",
    "description": "This is my package sunburst-chart",
    "keywords": [
        "tsetis",
        "laravel",
        "sunburst-chart"
    ],
    "homepage": "https://github.com/tsetis/sunburst-chart",
    "support": {
        "issues": "https://github.com/tsetis/sunburst-chart/issues",
        "source": "https://github.com/tsetis/sunburst-chart"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stelios Tsetis",
            "email": "steliost@o44.gr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Tsetis\\SunburstChart\\": "src/",
            "Tsetis\\SunburstChart\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tsetis\\SunburstChart\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tsetis\\SunburstChart\\SunburstChartServiceProvider"
            ],
            "aliases": {
                "SunburstChart": "Tsetis\\SunburstChart\\Facades\\SunburstChart"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}